<view class="{{['uni-list-item',disabled?'uni-list-item--disabled':'']}}" hover-class="{{disabled||showSwitch?'':'uni-list-item--hover'}}" data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" bindtap="__e"><view class="{{['uni-list-item__container',contain?'contain':'']}}" style="{{(newStyle)}}" hover-class="a"><block wx:if="{{thumb}}"><view class="uni-list-item__icon"><image class="uni-list-item__icon-img" src="{{thumb}}"></image></view></block><block wx:else><block wx:if="{{showExtraIcon}}"><view class="uni-list-item__icon d-flex j-center"><block wx:if="{{!otherIcon}}"><uni-icon vue-id="39210e02-1" color="{{extraIcon.color}}" size="{{extraIcon.size}}" type="{{extraIcon.type}}" bind:__l="__l"></uni-icon></block><block wx:if="{{otherIcon}}"><view class="{{['icon','iconfont',otherIcon]}}" style="{{(otherIconStyle)}}"></view></block><block wx:if="{{!leftIcon}}"><uni-icon vue-id="39210e02-2" color="{{extraIcon.color}}" size="{{extraIcon.size}}" type="{{extraIcon.type}}" bind:__l="__l"></uni-icon></block><block wx:if="{{leftIcon}}"><view class="{{['iconfont',leftIcon]}}" style="{{(leftIconStyle)}}"></view></block></view></block></block><view class="uni-list-item__content"><view class="uni-list-item__content-title" style="{{(titleStyle)}}">{{''+title+''}}<slot name="title"></slot></view><block wx:if="{{showNote||note}}"><view class="uni-list-item__content-note">{{''+note+''}}<slot name="note"></slot></view></block><slot></slot></view><block wx:if="{{showRightSlot||showBadge||showArrow||showSwitch}}"><view class="uni-list-item__extra" style="{{(extraStyle)}}"><slot name="rightContent"></slot><block wx:if="{{$slots.right}}"><slot name="right"></slot></block><block wx:else><block wx:if="{{showArrow&&showArrowIcon}}"><uni-icon class="iconfont icon-you" style="color:rgba(0,0,0,.3);font-size:26rpx;margin-top:5px;" vue-id="39210e02-3" bind:__l="__l"></uni-icon></block><block wx:if="{{showBadge}}"><uni-badge vue-id="39210e02-4" type="{{badgeType}}" text="{{badgeText}}" bind:__l="__l"></uni-badge></block><block wx:if="{{showSwitch}}"><switch disabled="{{disabled}}" checked="{{switchChecked}}" data-event-opts="{{[['change',[['onSwitchChange',['$event']]]]]}}" bindchange="__e"></switch></block></block></view></block></view></view>